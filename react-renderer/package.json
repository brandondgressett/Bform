{
  "name": "@bformdomain/react-renderer",
  "version": "1.0.0",
  "description": "React library for rendering BFormDomain entities with Bootstrap layouts, JSON Schema forms, and AG-Grid tables",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "typedoc --watch",
    "docs:build": "typedoc"
  },
  "keywords": [
    "react",
    "bformdomain",
    "forms",
    "tables",
    "bootstrap",
    "json-schema",
    "ag-grid",
    "enterprise"
  ],
  "author": "BFormDomain Team",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@ag-grid-community/react": "^31.0.0",
    "@ag-grid-community/core": "^31.0.0",
    "@hookform/resolvers": "^3.3.0",
    "@rjsf/core": "^5.15.0",
    "@rjsf/utils": "^5.15.0",
    "@rjsf/validator-ajv8": "^5.15.0",
    "@rjsf/bootstrap-4": "^5.15.0",
    "bootstrap": "^5.3.0",
    "classnames": "^2.3.2",
    "react-hook-form": "^7.48.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/blocks": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/testing-library": "^0.2.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.6.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.17.0",
    "typescript": "^5.3.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/user-event": "^14.5.0",
    "prettier": "^3.1.0",
    "eslint-config-prettier": "^9.1.0",
    "rimraf": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bformdomain/react-renderer.git"
  },
  "bugs": {
    "url": "https://github.com/bformdomain/react-renderer/issues"
  },
  "homepage": "https://github.com/bformdomain/react-renderer#readme"
}